{
  "/api/v1/*": "/$1",
  "/auth/request": "/auth-request",
  "*?isLatestFirst=false*": "$1-asc$2",
  "*?isLatestFirst=true*": "$1-desc$2",
  "*&pageSize=5*": "$1-5$2",
  "*&pageSize=10*": "$1-10$2",
  "*&pageSize=20*": "$1-20$2",
  "*&bookmark=*": "$1",
  "/document/:id/metadata": "/document-metadata/:id",
  "/identity/documents/list*": "/identity-documents-list$1"
}